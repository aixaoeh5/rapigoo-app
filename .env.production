# Production Environment Variables for RapiGoo
NODE_ENV=production

# API Configuration
API_URL=https://api.rapigoo.com
API_TIMEOUT=30000

# Performance Optimizations
ENABLE_FLIPPER=false
ENABLE_HERMES=true
ENABLE_PROGUARD=true

# Bundle Optimizations
BUNDLE_COMMAND=bundle
METRO_CACHE=true
ASSET_OPTIMIZATION=true

# Logging
LOG_LEVEL=error
DEBUG_MODE=false

# Analytics
ANALYTICS_ENABLED=true